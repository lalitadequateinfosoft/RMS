
@{
    Layout = null;
}

<!DOCTYPE HTML>
<html>


<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>RMS</title>

    <link rel="shortcut icon" href="~/SiteContent/favicon.png">
    <link rel="apple-touch-icon" href="~/SiteContent/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/SiteContent/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/SiteContent/apple-touch-icon-114x114.png">

    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,700,700italic' rel='stylesheet' type='text/css'>

    <link href="~/SiteContent/css/style.css" rel="stylesheet" media="screen" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet" />
</head>
<body>

    <div class="loader">
        <div class="loader-brand"><img alt="" class="img-responsive center-block" src="~/SiteContent/img/brand-white.png"></div>

    </div>
    <div id="layout" class="layout">

        <header id="top" class="navbar js-navbar-affix">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="#layout" class="brand js-target-scroll">
                        <p style="font-size: 26px;color: #ec0505;"><strong>Restaurant Booking</strong> </p>
                    </a>
                </div>
                <div class="collapse navbar-collapse" id="navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="active"><a href="/Home/index">Home</a></li>
                        <li><a href="@Url.Action("Login","Admin",new {Area="Admin" })">Admin</a></li>
                        <li><a href="@Url.Action("Booking","Restaurant",new {Area="Restaurant" })">Book Now</a></li>
                    </ul>
                </div>
            </div>
        </header>

        <main id="home" class="masthead masked">
            <div class="opener rel-1">
                <div class="container">
                    <div class="row">
                        <div class="lead col-lg-6">
                            <h1 class="wow fadeInDown">Restaurant Management System.</h1>
                            <div class="lead-controls">
                                <a href="@Url.Action("Booking","Restaurant",new {Area="Restaurant" })" class="btn-lead btn" data-toggle="modal">Get start now</a>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>



        <footer id="footer" class="footer text-center">
            <div class="container">
                <div class="social">
                    <a href="#" class="fa fa-2x fa-facebook"></a>
                    <a href="#" class="fa fa-2x fa-twitter"></a>
                    <a href="#" class="fa fa-2x fa-pinterest"></a>
                    <a href="#" class="fa fa-2x fa-youtube-play"></a>
                </div>
            </div>
            <div class="copy">
                © 2021 Lalit Yadav. All rights reserved by <a href="/Home/Index" target="_blank">RMS</a>
            </div>
        </footer>
    </div>

    

    <script data-cfasync="false" src="~/SiteContent/email-decode.min.js"></script>
    <script src="~/SiteContent/js/jquery.min.js"></script>
    <script src="~/SiteContent/js/bootstrap.min.js"></script>
    <script src="~/SiteContent/js/smoothscroll.js"></script>
    <script src="~/SiteContent/js/wow.min.js"></script>
    <script src="~/SiteContent/js/jquery.magnific-popup.min.js"></script>
    <script src="~/SiteContent/js/owl.carousel.min.js"></script>
    <script src="~/SiteContent/js/jquery.validate.min.js"></script>
    <script src="~/SiteContent/js/jquery.stellar.min.js"></script>
    <script src="~/SiteContent/js/jquery.easypiechart.min.js"></script>
    <script src="~/SiteContent/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/SiteContent/js/interface.js"></script>

</body>

